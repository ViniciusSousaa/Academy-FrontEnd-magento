<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="content" >
              <block class="Magento\Cms\Block\Block" name="banner-slide-home">
                    <arguments>
                        <argument name="block_id" xsi:type="string">banner-slider-home</argument>
                    </arguments>
                </block>
                <block name="content-teste-home" template="Magento_Theme::html/content-teste-home.phtml" after="banner-slider-home" />
                <block name="buy-by-categories" template="Magento_Theme::html/buy-by-categories.phtml" />
        </referenceContainer>
    </body>
</page>
